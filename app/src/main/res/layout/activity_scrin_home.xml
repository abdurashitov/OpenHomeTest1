<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ScrinHomeActivity">

    <Button
        android:id="@+id/button3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/ic_chevron_left"
        android:onClick="back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="24dp"
        android:background="@color/Gainsboro"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3" />


    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/circleImageView"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:onClick="openUser1Profile"
        android:src="@drawable/c2bgaoydqjo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view">

    </de.hdodenhof.circleimageview.CircleImageView>

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/circleImageView1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:onClick="openUser2Profile"
        android:src="@drawable/c2bgaoydqjo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view">

    </de.hdodenhof.circleimageview.CircleImageView>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="24dp"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@+id/circleImageView">

        <Button
            android:id="@+id/buttonGenerateDogovor"
            android:layout_width="250dp"
            android:layout_height="35dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/button_register_send"
            android:onClick="continue_btn"
            android:text="Сгенерировать договор "
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textSize="16sp"></Button>

        <Button
            android:id="@+id/laoadNewShablon"
            android:layout_width="250dp"
            android:layout_height="35dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/button_register_send"
            android:onClick="continue_btn"
            android:text="Загрузить шаблон договора"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textSize="16sp"></Button>

        <Button
            android:id="@+id/laoadNewDogovor"
            android:layout_width="250dp"
            android:layout_height="35dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/button_register_send"
            android:onClick="continue_btn"
            android:text="Загрузить готовый договор"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textSize="16sp"></Button>

        <Button
            android:id="@+id/CloseDogovor"
            android:layout_width="250dp"
            android:layout_height="35dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/button_register_send"
            android:onClick="continue_btn"
            android:text="Расторгнуть договор"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textSize="16sp"></Button>

        <Button
            android:id="@+id/btnOplata"
            android:layout_width="250dp"
            android:layout_height="35dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/button_register_send"
            android:onClick="continue_btn"
            android:text="Оплатить"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textSize="16sp"></Button>

    </LinearLayout>

    <View
        android:id="@+id/view2"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:background="@color/Gainsboro"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/montserrat_alternates_medium"
                android:text="История платежей"
                android:textAlignment="center"
                android:textColor="@color/Manatee"></TextView>

            <View
                android:id="@+id/textView21"
                android:layout_width="300dp"
                android:layout_height="150dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/add_new_home_btn"
                android:onClick="add_new_home"
                android:textAlignment="center"
                android:textColor="@color/Manatee" />
        </LinearLayout>

    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>